@model List<CourseResult>
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Trainee</th>
            <th scope="col">Course</th>
            <th scope="col">Degree</th>
            <th scope="col"></th>
            <th scope="col"></th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Trainee.Name</td>
                <td>@item.Course.Name</td>
                <td>@item.Degree</td>
                <td><a href="/courseresult/detail/@item.Id"><button type="button" class="btn btn-primary">Detail</button></a> </td>
                <td><a href="/courseresult/GetEditView/@item.Id"><button type="button" class="btn btn-primary">Edit</button></a> </td>
                <td><a href="/courseresult/DeletecourseResult/@item.Id"><button type="button" class="btn btn-danger">Delete</button></a> </td>
            </tr>
        }
    </tbody>
</table>
<a href="/courseresult/getview" style="margin-top:10px"> <button type="button" class="btn btn-primary">Add</button></a>

