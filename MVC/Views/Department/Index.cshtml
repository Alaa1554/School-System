@model List<Department>
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Manager</th>
            <th scope="col"></th>
            <th scope="col"></th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Manager</td>
                <td><a href="/department/detail/@item.Id"><button type="button" class="btn btn-primary">Detail</button></a> </td>
                <td><a href="/department/GetEditView/@item.Id"><button type="button" class="btn btn-primary">Edit</button></a> </td>
                <td><a href="/department/Deletedepartment/@item.Id"><button type="button" class="btn btn-danger">Delete</button></a> </td>
            </tr>
        }
    </tbody>
</table>
<a href="/department/getview" style="margin-top:10px"> <button type="button" class="btn btn-primary">Add</button></a>